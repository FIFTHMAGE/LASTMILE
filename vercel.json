{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/test",
      "dest": "/frontend/public/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/test.html"
    }
  ]
}